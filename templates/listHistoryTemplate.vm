#set( $count=0)
<div class="ui-widget top-panel" id="patient_history">

#foreach( $category in $categories  )
		<br/>	
		<div class="ui-state-highlight ui-corner-all" style="padding: .3em;">

		<table cellpadding="0" cellspacing="0" border="0">
			
		<tr class="gradeX">
			<td value="$category.category"><b>$category.category</b></td>
			
			#foreach( $symptom in $category.symptoms  )
				<tr><td value="$symptom.name">$symptom.name</td><td><input type='checkbox' value="{$symptom.name}_chkbx"></input></td></tr>
			#end
			
		</tr>
		</table>
		</div>

#end
</tbody>
</div>