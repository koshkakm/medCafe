<table cellpadding="0" cellspacing="0" border="0" class="display" id="Medications$patient_id">
<thead><tr><th></th><th></th><th></th></tr></thead><tbody>
#foreach( $medication in $medications )
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Manufacturer Info</td><td value="$medication.medicationInformation.manufacturedMaterial.freeTextBrandName">$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td></tr>
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Patient Information</td><td value="$medication.patientInstructions">$medication.patientInstructions</td></tr>
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Effective Time</td><td value="$medication.effectiveTime.value">$medication.effectiveTime.value</td></tr>
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Dose</td><td value="$medication.dose.value">$medication.dose.value</td></tr>
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Delivery Method</td><td value="$medication.deliveryMethod.value">$medication.deliveryMethod.value</td></tr>
<tr class="gradeX"><td>$medication.medicationInformation.manufacturedMaterial.freeTextBrandName</td><td>Narrative</td><td value="$medication.narrative">$medication.narrative</td></tr>
#end
</tbody>
<table>
